/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=invSel);
    
    And(a=a, b=invSel, out=aAndNotSel);
    
    And(a=b, b=sel, out=bAndSel);

    Or(a=aAndNotSel, b=bAndSel, out=out);
}